@model Frontend_Sistema_Votaciones.Models.PartidoViewModel

@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1 class="text-center">Detalles</h1>


<div class="card mt-3">
    <div class="card-header text-center">
        Información del Partido
    </div>
    <div class="card-body">
        <dl class="row">

            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Part_Id)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Part_Id)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Part_Descripcion)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Part_Descripcion)
            </dd>

            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Part_Imagen)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Part_Imagen)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Part_Color)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Part_Color)
            </dd>
        </dl>
    </div>
    </dl>
</div>
</div>

<hr />
<div>
    <a class="btn btn-primary" href="@Url.Action("Edit", new { id = Model.Part_Id })">Editar</a>
    <a class="btn btn-secondary" href="@Url.Action("Index")">Regresar</a>
</div>

<hr />
<div class="card">
    <div class="card-header text-center">
        Auditoría
    </div>
    <div class="card-body">
        <dl class="row">
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Part_UsuarioCreacion)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Part_UsuarioCreacion)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Part_FechaCreacion)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Part_FechaCreacion)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Part_UsuarioModifica)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Part_UsuarioModifica)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Part_FechaModifica)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Part_FechaModifica)
            </dd>
            <dt class="col-sm-2">
                @Html.DisplayNameFor(model => model.Part_Estado)
            </dt>
            <dd class="col-sm-10">
                @Html.DisplayFor(model => model.Part_Estado)
            </dd>
        </dl>
    </div>
    </dl>
</div>


