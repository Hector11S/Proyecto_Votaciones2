                    </dd>
                </dl>
            </div>
        </dl>
    </div>
</div>

<hr />
<div>
    <a class="btn btn-primary" href="@Url.Action("_Edit", new { id = Model.Muni_Codigo })">Editar</a>
    <a class="btn btn-secondary" href="@Url.Action("Index")">Regresar</a>
</div>

<hr />
