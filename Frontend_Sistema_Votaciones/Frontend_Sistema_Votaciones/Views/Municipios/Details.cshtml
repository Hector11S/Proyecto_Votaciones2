@model Frontend_Sistema_Votaciones.Models.MunicipioViewModel

@{
    ViewData["Title"] = "Details";
    Layout = "~/Views/Shared/_Layout.cshtml";
}

<h1 class="text-center">Detalles</h1>


<div class="card mt-3">
    <div class="card-header text-center">
        Información del Municipio
    </div>
    <div class="card-body">
        <dl class="row">
            <div class="col-sm-6">
                <dl>
                    <dt>
                        @Html.DisplayNameFor(model => model.Muni_Codigo)
                    </dt>
                    <dd>
                        @Html.DisplayFor(model => model.Muni_Descripcion)
                    </dd>
                    <dd>
                        @Html.DisplayFor(model => model.Dept_Codigo)
